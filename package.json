{
  "name": "@comake/skl-app-server",
  "version": "0.3.1",
  "description": "Runs a Node.js server using componentsjs preset with configurations for SKL.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comake/skl-app-server.git"
  },
  "keywords": [
    "skl",
    "linked-data",
    "rdf",
    "jsonld"
  ],
  "author": {
    "name": "Adler Faulkner",
    "email": "adler@comake.io"
  },
  "bugs": {
    "url": "https://github.com/comake/skl-app-server/issues"
  },
  "homepage": "https://github.com/comake/skl-app-server#readme",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@comake/skl-app-server",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/skl-app-server/^0.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/skl-app-server/^0.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/skl-app-server/^0.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/skl-app-server/^0.0.0/dist/": "dist/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "config",
    "assets"
  ],
  "bin": {
    "skl-app-server": "bin/server.js",
    "skl-app-server-drop-storages": "bin/drop-storages.js",
    "skl-app-server-seed-storages": "bin/seed-storages.js",
    "skl-app-server-delete-queue": "bin/delete-queue.js",
    "skl-app-server-delete-all-queues": "bin/delete-all-queues.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "start": "node ./bin/server.js",
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -r sklas -i .componentsignore --typeScopedContexts",
    "build:ts": "tsc",
    "lint": "eslint . --cache --ignore-path .gitignore --max-warnings 0",
    "jest": "jest --coverage",
    "test": "npm run test:ts && npm run jest",
    "test:deploy": "chmod +x ./test/deploy/validate-package.sh && ./test/deploy/validate-package.sh",
    "test:ts": "tsc -p test --noEmit",
    "test:integration": "jest --coverageReporters text-summary -- test/integration",
    "test:unit": "jest --config=./jest.coverage.config.js test/unit",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@tsconfig/node14": "^1.0.3",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.1",
    "@types/end-of-stream": "^1.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.6.1",
    "@types/pump": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/url-join": "^4.0.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "componentsjs-generator": "^2.6.1",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.18.0",
    "eslint-config-es": "^3.26.13",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "node-mocks-http": "^1.11.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "arrayify-stream": "^2.0.0",
    "bull": "^4.8.5",
    "componentsjs": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "end-of-stream": "^1.4.4",
    "escape-string-regexp": "^4.0.0",
    "fs-extra": "^10.1.0",
    "mime-types": "^2.1.35",
    "path-to-regexp": "^6.2.1",
    "pg": "^8.7.3",
    "pump": "^3.0.0",
    "typeorm": "^0.3.7",
    "url-join": "^4.0.1",
    "winston": "^3.8.1",
    "winston-transport": "^4.5.0",
    "ws": "^8.8.1",
    "yargs": "^17.5.1"
  }
}
