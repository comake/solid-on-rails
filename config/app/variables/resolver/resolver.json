      "@id": "urn:skl-app-server-setup:default:SettingsResolver",
      "@type": "CombinedSettingsResolver",
      "resolvers": [
        {
          "CombinedSettingsResolver:_resolvers_key": "urn:skl-app-server:default:variable:baseUrl",
          "CombinedSettingsResolver:_resolvers_value": {
            "@type": "BaseUrlExtractor"
          }
        },
        {
          "CombinedSettingsResolver:_resolvers_key": "urn:skl-app-server:default:variable:loggingLevel",
          "CombinedSettingsResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "loggingLevel",
            "defaultValue": "info"
          }
        },
        {
          "CombinedSettingsResolver:_resolvers_key": "urn:skl-app-server:default:variable:modulePathPlaceholder",
          "CombinedSettingsResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "modulePathPlaceholder",
            "defaultValue": "sklAppServer"
          }
        },
        {
          "CombinedSettingsResolver:_resolvers_key": "urn:skl-app-server:default:variable:port",
          "CombinedSettingsResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "port",
            "defaultValue": 3000
          }
        },
        {
          "CombinedSettingsResolver:_resolvers_key": "urn:skl-app-server:default:variable:rootFilePath",
          "CombinedSettingsResolver:_resolvers_value": {
            "@type": "AssetPathExtractor",
            "key": "rootFilePath",
            "defaultPath": "./",
            "pathResolver": { "@id": "urn:skl-app-server:default:PathResolver" }
          }
        },
        {
          "CombinedSettingsResolver:_resolvers_key": "urn:skl-app-server:default:variable:showStackTrace",
          "CombinedSettingsResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "showStackTrace",
            "defaultValue": false
          }
        }
      ]
    }
